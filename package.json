{
  "name": "@alex-programmer/lightdb",
  "type": "module",
  "version": "0.0.1-alpha",
  "packageManager": "pnpm@8.7.0",
  "description": "Lightweight caching of local data",
  "author": "Alex-Programmer-Bro <alex.lichangnan@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/Alex-Programmer-Bro/lightdb#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alex-Programmer-Bro/lightdb.git"
  },
  "bugs": "https://github.com/Alex-Programmer-Bro/lightdb/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "debug": "^4.3.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.0",
    "@antfu/ni": "^0.21.6",
    "@antfu/utils": "^0.7.6",
    "@types/debug": "^4.1.8",
    "@types/node": "^20.5.7",
    "@vitest/coverage-v8": "^0.34.5",
    "bumpp": "^9.2.0",
    "eslint": "^8.48.0",
    "esno": "^0.17.0",
    "lint-staged": "^14.0.1",
    "pnpm": "^8.7.0",
    "rimraf": "^5.0.1",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vite": "^4.4.9",
    "vitest": "^0.34.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
